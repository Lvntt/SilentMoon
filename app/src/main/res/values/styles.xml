    <style name="weekday_chip">
        <item name="android:checkable">true</item>
        <item name="chipCornerRadius">50dp</item>
        <item name="textStartPadding">0dp</item>
        <item name="textEndPadding">0dp</item>
        <item name="chipMinTouchTargetSize">0dp</item>
        <item name="chipBackgroundColor">@color/weekday_background_color</item>
        <item name="chipStrokeColor">@color/weekday_stroke_color</item>
        <item name="android:layout_width">41dp</item>
        <item name="android:layout_height">41dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/weekday_text_color</item>
        <item name="android:textAppearance">@style/weekdayLabel</item>
